class Clase:
    def __init__(self, atributo):
        
        self.atributo = atributo

    def metodo(self):
        return "El atributo es: " + self.atributo
    
    def imprimir(self):
        print("Imprimiendo: " + self.atributo )

instancia = Clase("valor")
print(instancia.metodo() )
instancia.imprimir()
